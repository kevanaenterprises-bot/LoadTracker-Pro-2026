# Database connection string for Railway PostgreSQL
DATABASE_URL=postgresql://user:password@host:port/database

# API Server URL (for frontend to connect to backend)
VITE_API_URL=http://localhost:3001

# HERE Maps API Key for geocoding and routing
# Get your API key from: https://developer.here.com/
HERE_API_KEY=your_here_api_key_here

# Google Cloud Vision API Key for OCR functionality
# Get your API key from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_CLOUD_VISION_API_KEY=your_api_key_here
